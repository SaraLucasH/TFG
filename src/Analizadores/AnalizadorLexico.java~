				;
            } 
            // fall through
          case 79: break;
          case 36: 
            { //Sumatorio	
				;
            } 
            // fall through
          case 80: break;
          case 37: 
            { acronimo=""; posibleLF="";
            } 
            // fall through
          case 81: break;
          case 38: 
            { String b=yytext();
			if(b!=null){
				yypushback(b.length());
				acWLf.clean();
			}
			yybegin(YYINITIAL);
            } 
            // fall through
          case 82: break;
          case 39: 
            { ;
            } 
            // fall through
          case 83: break;
          case 40: 
            { yybegin(estado2);
		System.out.println("pa");
            } 
            // fall through
          case 84: break;
          case 41: 
            { if(yytext()!=null){
			yypushback(yytext().length());
			acWLf.clean();}
			yybegin(YYINITIAL);
            } 
            // fall through
          case 85: break;
          case 42: 
            { System.out.println("Acronimo");
			acronimo=yytext();
			acWLf.setAcronimo(acronimo);
			yybegin(estado3);
			System.out.println("ac");
            } 
            // fall through
          case 86: break;
          case 43: 
            { yybegin(YYINITIAL); acWLf.clean();
            } 
            // fall through
          case 87: break;
          case 44: 
            { yybegin(YYINITIAL);
		System.out.println("pc");
		//Cuidado si le paso el objeto en el sintactico lo usa como puntero y solo se guarda la ultima ocurrencia. Por ello new Object
		return new Symbol(sym.acWithLf,yyline+1,yycolumn+1,new FormaLargaWithAc(acWLf.getAcronimo(),acWLf.getFormaLarga()));
            } 
            // fall through
          case 88: break;
          default:
            zzScanError(ZZ_NO_MATCH);
        }
      }
    }
